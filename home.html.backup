        $(document).ready(function() {
            var serverUrl = "https://www.searates.com";
            var requestUri = "/";
            var reg = "MX";

            var items = '';
            var lang_titles = {'en' : 'English (UK)', 'ru': 'Pусский', 'ua': 'Українська', 'de': 'Deutsch', 'es': 'Español', 'cn': '中文', 'fr': 'Français', 'ar': 'عربي', 'tr': 'Türkçe', 'it': 'Italiano', 'th': 'ไทย', 'ge': 'ქართული', 'ir': 'فارسی', 'kr': '한국어', 'jp': '日本語', 'il': 'עברית' , 'gr': 'Ελληνικά', 'pl': 'Polski', 'vi': 'Người việt nam', 'pt': 'Português'};

            for (var key in lang_titles) {
                items += '<li role="menuitem" class="countrylang-lang-item"><a href="javascript:;" data-region="' + reg + '" data-lang="' + key + '"><span aria-hidden="true" class="icon icon-chevron-right"></span> ' + lang_titles[key] + '</a></li>';
            }

            $('#js-countrylang-language').html(items);
            $('#countrylang-content')
                .on('click', '.countrylang-lang-item > a', function(){

                    var lang = $(this).data('lang');
                    var reg = $(this).data('region');

                    setCookie('region',reg,{
                        path: '/',
                        expires:3600*24*7,
                        domain: '.searates.com'
                    });


                    if (lang != 'en') document.location = serverUrl + '/' + lang + requestUri;
                    else document.location = serverUrl + requestUri;
                })
                .on('click', '.countrylang-country-item', function(){
                    var reg = $(this).data('country') || 'US';
                    setCookie('region',reg,{
                        path: '/',
                        expires:3600*24*7,
                        domain: '.searates.com'
                    });
                    document.location = serverUrl + requestUri;

                });

            //$('.countrylang-country-item[data-country="//"]').click();
        });



        function setCookie(name, value, options) {
            options = options || {};

            var expires = options.expires;

            if (typeof expires == "number" && expires) {
                var d = new Date();
                d.setTime(d.getTime() + expires * 1000);
                expires = options.expires = d;
            }
            if (expires && expires.toUTCString) {
                options.expires = expires.toUTCString();
            }

            value = encodeURIComponent(value);

            var updatedCookie = name + "=" + value;

            for (var propName in options) {
                updatedCookie += "; " + propName;
                var propValue = options[propName];
                if (propValue !== true) {
                    updatedCookie += "=" + propValue;
                }
            }
            document.cookie = updatedCookie + '; SameSite=Strict';
        }

